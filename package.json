{
  "name": "phit-frontend",
  "private": true,
  "license": "UNLICENSED",
  "author": "BlueSkyAI",
  "version": "0.4.7",
  "scripts": {
    "build": "next build",
    "codegen": "graphql-codegen --config codegen.ts",
    "dev": "next dev -p 3006 --turbo",
    "dev:webpack": "next dev -p 3006",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,css,json}\" && eslint --fix \"**/*.{ts,tsx}\"",
    "lint": "eslint --fix --report-unused-disable-directives \"app/**/*.{ts,tsx}\"",
    "lint:check": "eslint --report-unused-disable-directives \"app/**/*.{ts,tsx}\"",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,jsx,css,json}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,js,jsx,css,json}\"",
    "serve": "http-server dist -a localhost -p 3006",
    "start": "next dev -p 3006 --turbo",
    "clean:cache": "rm -rf .next node_modules/.cache",
    "typecheck": "tsc --project tsconfig.json",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "e2e:ci": "start-server-and-test dev http://localhost:3006 e2e"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "npm run prettier:check && npm run typecheck && npm run lint:check"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@headlessui/react": "^1.7.17",
    "@hookform/resolvers": "^3.3.1",
    "@mui/icons-material": "^5.x",
    "@mui/material": "^5.x",
    "@mui/x-date-pickers": "^8.10.2",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^4.42.0",
    "@typeform/embed-react": "^3.10.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react-datepicker": "^4.19.5",
    "axios": "^1.5.1",
    "class-variance-authority": "^0.7.0",
    "highcharts": "^11.1.0",
    "highcharts-react-official": "^3.2.1",
    "immer": "^10.0.3",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.330.0",
    "next": "^15.5.0",
    "next-themes": "^0.2.1",
    "notistack": "^3.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-otp-input": "^3.1.0",
    "react-select": "^5.7.7",
    "simplebar-react": "^3.2.4",
    "tailwind-merge": "^1.14.0",
    "yup": "^1.3.2",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.2",
    "@graphql-codegen/fragment-matcher": "^5.0.2",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@tsconfig/next": "^2.0.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-input-mask": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "autoprefixer": "^10",
    "cypress": "^15.3.0",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-prettier": "^5.1.2",
    "http-server": "^14.1.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "postcss-nesting": "^13.0.2",
    "prettier": "^3.1.1",
    "simple-git-hooks": "^2.13.1",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  }
}
