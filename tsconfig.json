{
  "extends": "@tsconfig/next/tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "target": "es5",
    "paths": {
      "~/*": ["./*"]
    },
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "next-env.d.ts",
    "dist/types/**/*.ts"
  ],
  "exclude": ["node_modules", ".next", "dist"]
}
